@model Fusioness.Models.Rotas.RotaModel
@{
  ViewBag.Title = "Index";
  Layout = "~/Views/Shared/LayoutBase.cshtml";
}

<!-- LINHA 1 -->
<div class="row-fluid">
  <!-- BLOCK -->
  <div class="span12">
    <div class="card">
      <h2 class="card-heading simple">Cabeçalho - Bloco tamanho 12</h2>
      <div class="card-body">
        @using (Html.BeginForm("QualificarRota", "Rota", FormMethod.Post))
        {
          @Html.LabelFor(m => m.RotasDoUsuario)
          @Html.DropDownListFor(m => m.RotaSelecionada.IdRota, new SelectList(Model.RotasDoUsuario, "IdRota", "IdRota"))
          <br />
          @Html.LabelFor(m => m.TiposDeRotas)
          @Html.DropDownListFor(m => m.TiposDeRotaSelecionada.IdTipoRota, new SelectList(Model.TiposDeRotas, "IdTipoRota", "Descricao"))
          <br />
          <input type="submit" value="Send" />
        }
      </div>
    </div>
  </div>
  <!-- END BLOCK -->
</div>
<!-- END LINHA 2 -->
