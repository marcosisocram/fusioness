@model Fusioness.Models.Account.AcessoModel
@{ Layout = "~/Views/Shared/_LayoutBaseAcesso.cshtml"; }
@using(Html.BeginForm("Entrar", "Acesso", FormMethod.Post))
{
    @Html.LabelFor(m => m.UsuarioLogado.Login)
    @Html.TextBoxFor(m => m.UsuarioLogado.Login)
    <br />
    @Html.LabelFor(m => m.UsuarioLogado.Senha)
    @Html.PasswordFor(m => m.UsuarioLogado.Senha)
    <br />
    <input type="submit" value="Acessar"/>
}