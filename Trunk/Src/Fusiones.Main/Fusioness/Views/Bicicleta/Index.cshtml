@model Fusioness.Models.Bicicletas.BicicletaModel
@{
  ViewBag.Title = "Index";
  Layout = "~/Views/Shared/LayoutBase.cshtml";
}

<!-- LINHA 1 -->
<div class="row-fluid">
  <!-- BLOCK -->
  <div class="span12">
    <div class="card">
      <h2 class="card-heading simple">Cabeçalho - Bloco tamanho 12</h2>
      <div class="card-body">
        @using (Html.BeginForm("InsertBicicleta", "Bicicleta", FormMethod.Post))
        {
          @Html.LabelFor(m => m.Bicicleta.Modelo)
          @Html.TextBoxFor(m => m.Bicicleta.Modelo)
          <br />
    
          @Html.LabelFor(m => m.Bicicleta.Marca)
          @Html.TextBoxFor(m => m.Bicicleta.Marca)
          <br />
          <input type="submit" value="Send" />
        }
      </div>
    </div>
  </div>
  <!-- END BLOCK -->
</div>
<!-- END LINHA 2 -->  